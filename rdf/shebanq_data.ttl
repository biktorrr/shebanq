@prefix    rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix     rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix   sq: <http://purl.org/collections/shebanq/> .
@prefix   oa: <http://www.w3.org/ns/oa#>.
@prefix      skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix      foaf: <http://xmlns.com/foaf/0.1/> .


#Schema
sq:Query rdfs:subClassOf oa:Annotation.
sq:Researcher rdfs:subClassOf foaf:Person.
sq:has_intension rdfs:subPropertyOf oa:hasBody.
sq:has_extension rdfs:subPropertyOf oa:hasTarget.
# sq:has_researcher 
# sq:made_query rdfs:subPropertyOf .

#Data

sq:researcher1 
	a sq:Researcher;
	foaf:name "Dirk Roorda";
	sq:made_query sq:query1.
	
sq:query1
	a sq:Query;
	sq:has_intension "select all * where @prefix is [myword]";
	sq:has_extension <http://shebanqserver.nl/word?w=12,3>;
	sq:has_researcher sq:researcher1.

	